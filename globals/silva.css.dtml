<dtml-let last_modified="_.DateTime()-14"
          expires="_.DateTime()+7"
          root="get_root_url()" >
<dtml-call "REQUEST.RESPONSE.setHeader( 'Content-Type', 'text/css' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Last-Modified', last_modified.toZone('GMT').rfc822() )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Cache-Control', 'max-age=36000, must-revalidate' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Expires', expires.toZone('GMT').rfc822() )">

/*
**  Silva stylesheet
**
**  The view crew at Infrae - all seasons of 2003-2005
**    Thanks to Alexander Limi (limi@plone.org) - October 2002
**      In turn, thanks to Geir Bækholt, Stian Søiland for input and guidance.
**
*/

/* {border: 1px dotted red; } */

<dtml-with stylesheet_properties>

body {
    background: &dtml-mainBackground;;
  background-image: url(<dtml-var root>/globals/skyrise.gif);
  background-repeat: repeat-x;
    font: &dtml-mainFont;;
    color: &dtml-mainFontColor;;
    margin: 0;
    padding: 0;
    scrollbar-base-color: &dtml-shade70;;
    scrollbar-highlight-color: &dtml-shade70;;
    scrollbar-track-color: &dtml-shade56;;
    scrollbar-darkshadow-color: &dtml-shade56;;
    scrollbar-3dlight-color: &dtml-mainTabBorderColorSelected;;
    scrollbar-shadow-color: &dtml-mainTabBorderColorSelected;;
    scrollbar-arrow-color: &dtml-mainTabFontColorSelected;;
}

div.workspace {
  background : &dtml-workspace;;
  margin: 0;
}

img {
  border: none;
  vertical-align: middle;
}

img.icon {
  margin: 0em;
}

a.icon:hover {
  text-decoration: none;
}


/* _________________________________________________________ path bar _____ */

div.pathBar {
  font-size: 90%;
    color: &dtml-shade36;;
    background-image: url(<dtml-var root>/globals/skylight.gif);
    background-repeat: repeat-x;
    background-color: &dtml-pathBarBackground;;
    padding: 0em 4px 0.2em 22px;
    border: none;
    line-height: 1.4em;
}

div.pathBar a {
    color: &dtml-shade26;;
    font-weight: &dtml-mainTabFontWeight;;
    margin: 0;
}
div.pathBar a:visited {
    color: &dtml-shade26;;
}
div.pathBar a:hover {
    color: &dtml-shade42;;
    text-decoration: underline;
}

div.pathBar a.manage {
  color: &dtml-shade50;;
  margin-left: 0.5em ! important;
}

div.pathBar a.idea {
  color: &dtml-shade78;;
}
div.pathBar a.idea:visited {
  color: &dtml-shade78;;
}
div.pathBar a.idea:hover {
  color: &dtml-shade96;;
  text-decoration: none;
}

div.pathBar img {
  vertical-align: top;
  margin: 0 0 0 -22px;
}


/* _______________________________________________________ matrix nav _____ */

div.tabs {
  display : block;
  height : 1.2em;
  background-image: url(<dtml-var root>/globals/skyrise.gif);
  background-repeat: repeat-x;
    background-color: &dtml-mainTabBackdrop;;
  margin : 0;
    padding: 0 16px 0 16px;
    border-width: 1px;
    border-style: none none solid none;
  border-bottom-color: &dtml-shade80;;
}

div.tabs a {
    /* The normal, unselected tabs. They are all links */
  float : left;
  display : block;
  width : 7.5em;
  height : 1.1em;
    color: &dtml-mainTabFontColor;;
    background: &dtml-mainTabBackground;;
    border-color: &dtml-mainTabBorderColor;;
    border-width: 1px;
    border-style: solid solid solid solid;
  border-bottom-color: &dtml-shade50;;
    margin: 0;
    padding: 0;
  text-align : center;
    font-weight: &dtml-mainTabFontWeight;;
}

div.tabs a:visited {
  color: &dtml-mainTabFontColor;;
  background: &dtml-mainTabBackground;;
}

div.tabs a:hover {
  color: &dtml-mainTabFontColorSelected;;
    background: &dtml-mainTabBackgroundSelected;;
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
}

div.tabs a.selected {
    /* The selected tab. There's only one of these */
    color: &dtml-mainTabFontColorSelected;;
    background: &dtml-mainTabBackgroundSelected;;
    border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.selected:visited {
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.recede {
  /* The parent tab of a subscreen. There might be one of these :) */
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundRecede;;
  border-bottom-color: &dtml-mainTabBackground;;
}

div.tabs a.recede:hover {
  background: &dtml-mainTabBackgroundSelected;;
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.left {
    /* The left side of the tab. */
  width : 1.5em;
    border-style: solid none solid solid;
}

div.tabs a.left:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.right {
    /* The right side of the tab. */
  width : 1.5em;
    border-style: solid solid solid none;
}

div.tabs a.right:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

/* help (available as accesskey link) */
div.tabs a.normal {
  float: right;
  width: 0em;
  display: none;
}

div.tabs img {
    margin: 0.3em 0 0 0;
  border: none;
  vertical-align : bottom;
}

div.tabs div.air {
  display : block;
  float : left;
  background : transparent;
  width : 2em;
  height : 1em;
}


/* _________________________________________ flow bar (or sidewalk :) _____ */

div.flowBar {
  background: &dtml-mainTabBackgroundSelected;;
  margin: 0;
  padding: 0;
  border-top: 3px solid &dtml-mainTabBackgroundSelected;;
  border-bottom: 1px solid #84a7a5;
  clear:both;
}


/* _____________________________________________________ middleground _____ */

div.middleground {
  display : block;
  height : 1.7em;
  background-color: #677a8c;
  margin : 0;
  padding: 0 17px 0 17px;
  border-bottom: 1px solid #456555;
}

div.middleground a {
  /* The normal, unselected buttons. They are all links */
  float : left;
  display : block;
  width : 12em;
  height : 1.3em;
  color: &dtml-transportFontColor;;
  background: #4d5680;
  margin: 1px 1em 1px 1.5em;
  padding: 0;
  border-color: #8faecf;
  border-width: 1px;
  border-style: solid solid solid solid;
  text-align : center;
  font-weight: &dtml-mainTabFontWeight;;
}

div.middleground a:visited {
  color: &dtml-transportFontColor;;
  background: #4d5680;
}

div.middleground a:hover {
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
}

div.middleground a.button {
  color: #d9f3f1;
  background: #065e32;
}

div.middleground a.button:visited {
  color: #d9f3f1;
  background: #065e32;
}

div.middleground a.button:hover {
  color: #424244;
  background: #b3d4bf;
}

div.middleground a.selected {
  /* A selected, highlighted button. There should only be one of these. */
  color: #424244;
  background: #b6d6f3;
  border-color: #3d4b67;
}

input.warning {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
  color : &dtml-destructiveFontColor;; 
  background : &dtml-destructiveBackground;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
  border: &dtml-destructiveBorder;;
    cursor: pointer;
}

/* ___________________________________________________________ footer _____ */

div.footer {
  clear: both;
  font-size: 94%;
  color: &dtml-shade30;;
  background: &dtml-listingTopBackground;;
  margin: 0.5em 0em 0em 0em;
    padding: 0em 16px 0.1em 16px;
    border-top: &dtml-shade60; 1px solid;
    border-bottom: none;
}

div.footer a {
  color: &dtml-mainLinkColor;;
  font-weight: &dtml-mainTabFontWeight;;
  padding: 0;
  margin: 0;
  text-decoration: none;
}
div.footer a:visited {
  color: &dtml-mainLinkColor;;
}

/* _____________________________________________________ personal bar _____ */

div.personalBar {
  text-align: center;
  background: #505e65;
  margin: 0em 0em 0em 0em;
  padding: 0.1em 16px 0.2em 16px;
  border-bottom: &dtml-shade30; 1px solid;
}

div.personalBar a {
  color: #b7cee6;
    padding: 0;
    margin: 0;
    text-decoration: none;
}
div.personalBar a:visited {
  color: #b7cee6;
}

div.personalBar img {
    vertical-align: middle;
}


/* ____________________________________________________ columns table _____ */

table {
  font-size: 100%; /* for IE 5.x */
}

table.columns {
    width: 100%;
    padding: 0;
}

table.columns tr {
  vertical-align: top;
}

/* 3 column table (default) */
table.columns td.left {
    width: 20%;
    padding: 0 7px 0 6px;
    vertical-align: top;
}
table.columns td.main {
    width: 60%;
    padding: 0 6px 0 0;
    vertical-align: top;
}
table.columns td.right {
    width: 20%;
    padding: 0 6px 0 0;
    vertical-align: top;
}

/* 2 column table */
table.columns2 td.left {
  width: 20%;
  padding: 0 0 0 6px;
  vertical-align: top;
}
table.columns2 td.main {
  width: 80%;
  padding: 0 6px 0 7px;
  vertical-align: top;
}

/* 1 column */
table.columns1 td.main {
  width: 100%;
  padding: 0 6px 0 6px;
  vertical-align: top;
}


/* ____________________________________________________ listing table _____ */

table.listing {
    color: #444444;
    width: 100%;
    margin: 0em 0em 2em 0em;
    border-left: &dtml-listingBorder;;
    border-bottom: &dtml-listingBorder;;
    border-collapse: collapse;
}

table.listing img {
    vertical-align: middle;
}

/* _____________________ rows ______________________ */

table.listing tr.controls {
  color: #c9dfe2;
  background: &dtml-listingControlsBackground;;
  text-align : right;
  /* in case it's empty */
  height : 0.2em;
}

table.listing tr.top-controls {
  background: &dtml-listingTopControlsBackground;;
  /* in case it's empty */
  height : 0.2em;
}

table.listing tr.odd {
    background: &dtml-listingOddBackground;;
}

/* applies only to mozilla comp. browsers */
td.main>table.listing tr.odd:hover td,
td.main>table.listing tr.even:hover td {
    background-color: #e5f2ff;
  }

table.listing tr.inter-controls td {
  color: #c9dfe2;
  background: &dtml-listingInterControlsBackground;;
}

tr.divider, th.divider, td.divider {
  height : 0.5em;
  background: &dtml-listingDividerBackground;;
}

/* _____________________ cells _____________________ */

table.listing th {
    color: &dtml-listingHeaderFontColor;;
    background: &dtml-listingHeaderBackground;; 
    padding: 0em 0.6em 0em 0.6em;
  border-top: none;
    border-bottom: &dtml-listingBorder;;
    border-right: &dtml-listingBorder;;
    font-weight: normal;
    text-align: left;
    white-space: nowrap;
}

table.listing th.listing-left {
  width: 2%;
  text-align: left;
  padding-left: 0.4em;
}

table.listing th.listing-right {
  width: 4%;
  text-align: right;
  padding-right: 0.4em;
}

table.listing th.align-left, table.listing td.align-left {
  text-align: left;
}
table.listing th.align-center, table.listing td.align-center {
  text-align: center;
}
table.listing th.align-right, table.listing td.align-right {
  text-align: right;
}

table.listing td {
    border-right: &dtml-listingBorder;;
    padding: 0em 0.6em;
  vertical-align: top;
  text-align: left;
}

/* cycle text color */
table.listing tr.even td {
  color: #333;
  background: &dtml-listingEvenBackground;;
}

/* _______ title cell _______ */
table.listing td.top {
  color: &dtml-shade40;;
  background: &dtml-listingTopBackground;;
  border: &dtml-listingBorder;;
  padding: 0em;
}
table.listing td.top h2 {
  color: &dtml-listingTopHeadingColor;;
  text-align: left;
  margin: 0.1em 0.4em;
  border: none;
}
table.listing td.top img {
  vertical-align: top;
}

table.listing tr.top-controls td {
  padding: 0em 0.4em;
}

div.controls {
  padding: 0.2em 0 0.2em 0
}

/* _______ subhead cell _______ */
table.listing tr.top-controls h3 {
  color: &dtml-listingTopControlsHeadingColor;;
  margin: 0.1em 0.2em 0.2em 0.2em;
  padding: 0em;
  border: none;
  line-height : 100%;
}

table.listing td.listing-left {
  text-align: left;
  padding: 0em 0.4em;
  white-space: nowrap;
}

table.listing td.listing-right {
  white-space: nowrap;
}

table.listing tr.controls td, tr.inter-controls td {
  padding: 0em 0.4em;
}

table.listing td.field-title {
  text-align : right;
}

/* 
 * some usability fixes
 * create a displayed block of the links, because then you can click on
 * the whole tablecell and don't have to focus the text
 */
 
table#sortable tbody a {
  display: block;
}

table#sortable a.icon img {
  float: left;
  margin-right: 0.5em;
}

/* reset the margin for the up/down arrows */
table#sortable td.listing-right a.icon img {
    margin: 0;
}

/* rename this selector */
.widebody {
  color: #c9dfe2;
}


/* ______________ sidebar navigation ______________ */

table.listing th.top {
  color: #e3eef0;
  background: &dtml-listingHeaderBackground;; 
  padding: 0;
  border-top: &dtml-listingBorder;;
  vertical-align: bottom;
  text-align: center;
}

table.listing th.top img {
  vertical-align: bottom;
  padding: 0.5em 0 0 0;
}

table.listing th.top a.normal:link {
  display : block;
    color: &dtml-mainTabFontColor;;
    background: &dtml-listingHeaderBackground;; 
}

table.listing th.top a.normal:visited {
  display : block;
    color: &dtml-mainTabFontColor;;
    background: &dtml-listingHeaderBackground;; 
}

table.listing th.top a.normal:hover {
  display : block;
    color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
    text-decoration: none;
}

/* _______ uplinks _______ */
table.listing a.top {
  display : block;
  width : 1.4em;
  background: &dtml-listingHeaderBackground;; 
    float: left;
    margin: 0em;
    border-right: &dtml-listingBorder;;
    padding: 0em 0.2em;
}

table.listing a.up {
  display : block;
  width : 1.4em;
  background: &dtml-listingHeaderBackground;; 
    float: right;
    margin: 0em;
    border-left: &dtml-listingBorder;;
    padding: 0em 0.2em;
}

table.listing a.top:hover {
    color: &dtml-mainTabBackgroundSelected;;
    background: &dtml-mainTabBackgroundSelected;;
    border-style: solid solid none none;
    text-decoration: none;
}

table.listing a.up:hover {
    color: &dtml-mainTabBackgroundSelected;;
    background: &dtml-mainTabBackgroundSelected;;
    border-style: solid none none solid;
  text-decoration: none;
}

td.selected {
  background: &dtml-oddRowBackground;;
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: &dtml-listingBorder;;
  border-bottom: &dtml-listingBorder;;
}

td.unselected {
  background: &dtml-evenRowBackground;;
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: 1px solid &dtml-evenRowBackground;;
  border-bottom:  1px solid &dtml-evenRowBackground;;
}

td.unselected:hover { 
  color: &dtml-mainTabFontColorSelected;;
  background-color: #dae7fa;
  cursor : pointer;
} 

table.listing td a.normal:link {
  display : block;
    color: &dtml-mainTabFontColor;;
    background: &dtml-listingHeaderBackground;; 
}

table.listing th.top a.normal:visited {
  display : block;
    color: &dtml-mainTabFontColor;;
    background: &dtml-listingHeaderBackground;; 
}

table.listing th.top a.normal:hover {
  display : block;
    color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
    text-decoration: none;
}

/* _______ slidelinks _______ */
table.listing a.left {
  display : block;
  width : 8px;
  height : 16px;
  float: left;
}

table.listing a.right {
  display : block;
  width : 8px;
  height : 16px;
  float: right;
}

table.listing a.left:hover {
  background-image: url(<dtml-var root>/globals/slideleft.gif);
  background-color: transparent;
  text-decoration: none;
}

table.listing a.right:hover {
  background-image: url(<dtml-var root>/globals/slideright.gif);
  background-color: transparent;
  text-decoration: none;
}

div.normal {
  display: block;
}


/* _____________________________________________________ focus shapes _____ */

table.sublisting {
  margin-top : 1em;
}

table.interlisting {
  margin : 0;
}

/* used to shape non-nested listing tables */
.focus-box {
  /* for IE5 */
  width : 73%;
  max-width : 56em; 
  margin : 0 7px 0 7px;
}

.focus-rectangle {
  /* for IE5 */
  width : 96%; /* for IE6 */
  max-width : 98%;
  margin : 0 7px;
  padding-top: 7px;
}


/* _______________________________________________________ form stuff _____ */

form {
    border: none;
    margin: 0;
}

input {
    color : &dtml-inputFontColor;;
  background : &dtml-inputBackground;;
  margin-top: 1px;
    margin-bottom: 1px; /* IE bug fix */
    padding: 0.1em;
    border: &dtml-inputBorder;;
    vertical-align: middle;
    font: &dtml-inputFont;;
}
 
textarea {
    font: &dtml-inputFont;;
    color : &dtml-inputFontColor;;
  background : &dtml-inputBackground;;
    padding: 0.1em;
    border: &dtml-inputBorder;;  
}

textarea.readable {
  font: 100% Verdana, Helvetica, Arial, sans-serif;
}

input.store, select.store {
  font: &dtml-inputFont;;
  color : &dtml-inputFontColor;;
  background : &dtml-inputBackground;;
  width: 90%;
  margin-top: 1px;
  margin-bottom: 1px; /* IE bug fix */
  padding: 0.1em;
  border: &dtml-inputBorder;;
}

/* 
* this selector works only for standard compatible browsers like mozilla, so
* IE ignores this definition below
*/
td>textarea.fixIE6widget, td>input.fixIE6widget[type=text], td>div.fixIE6widget, 
  td>input.store, td>textarea.store, td>select.store {
  width: 100%;
}

select {
    font: &dtml-inputFont;;
  font-size: 90%;
  color : &dtml-mainTabFontColorSelected;;
  background : &dtml-inputBackground;;
  margin: 0.1em;
  border : 1px solid &dtml-nightShade;;
    vertical-align: top;
  line-height : 96%;
}

select.store {
  font-size: 90%;
  line-height : 96%;
}

optgroup {
  font-size: 100%;
}

select.transport {
  color: &dtml-transportFontColor;;
  background : &dtml-transportBackground;;
}

select.manipulator {
  color: &dtml-manipulatorFontColor;;
  background : &dtml-manipulatorBackground;;
}

input[type="checkbox"] {
  color : &dtml-shade56;;
  background : &dtml-inputBackground;;
  border : 1px solid &dtml-inputBorder;;
}

/* checkboxes used (only) for deletion */
input.warning-checkbox {
  color : &dtml-shade56;;
  background : #ead5d3;
}

input.button {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
    color: &dtml-manipulatorFontColor;;
  background : &dtml-manipulatorBackground;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
  border: &dtml-manipulatorBorder;;
    cursor: pointer;
}

button.button {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
    color: &dtml-manipulatorFontColor;;
  background : &dtml-manipulatorBackground;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
  border: &dtml-manipulatorBorder;;
    cursor: pointer;
}

input.transport {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
  color: &dtml-transportFontColor;;
  background: &dtml-transportBackground;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
  border: &dtml-transportBorder;;
    cursor: pointer;
}

input.warning {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
  color : &dtml-destructiveFontColor;; 
  background : &dtml-destructiveBackground;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
  border: &dtml-destructiveBorder;;
    cursor: pointer;
}

input.cancel {
  font-size: 95%;
    font-weight: &dtml-buttonWeight;;
  color : &dtml-cancelFontColor;;
  background : #666668;
  background : &dtml-cancelBackground;;
  border : &dtml-cancelBorder;;
    margin: 0.2em 0.2em 0.1em 0.2em;
    padding: &dtml-manipulatorPadding;;
    cursor: pointer;
}

input.executor {
  font-size: 95%;
  font-weight: normal;
  color: #b3fffb;
  background : #284d5b;
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: 0 0.2em;
  border: 1px solid #6bb591;
  cursor: pointer;
}

input.executor[type="submit"]:hover { 
  color : #e5ffec; 
  background : #4393a3; 
  border-color : #8dc1a8;
}
/* publish screen state coded buttons */
input.reject {
  font-size: 95%;
  font-weight: &dtml-buttonWeight;;
  color: &dtml-manipulatorFontColor;;
  background : #8f7f01;
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: &dtml-manipulatorPadding;;
  border: &dtml-manipulatorBorder;;
  cursor: pointer;
}
input.revoke {
  font-size: 95%;
  font-weight: &dtml-buttonWeight;;
  color: &dtml-manipulatorFontColor;;
  background : #008c55;
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: &dtml-manipulatorPadding;;
  border: &dtml-manipulatorBorder;;
  cursor: pointer;
}
input.close {
  font-size: 95%;
  font-weight: &dtml-buttonWeight;;
  color: &dtml-manipulatorFontColor;;
  background : #202080;
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: &dtml-manipulatorPadding;;
  border: &dtml-manipulatorBorder;;
  cursor: pointer;
}


/* _______ form labels _______ */

label {
  color : #403f80; 
}

tr.controls label, tr.inter-controls label {
  color: &dtml-listingHeaderFontColor;;
}

th label {
  color: &dtml-listingHeaderFontColor;;
}


/* _______ pseudo form button selectors, matches button style _______ */

table.listing a.button:link {
  font-size: 100%;
  font-weight: &dtml-buttonWeight;;
  color: &dtml-manipulatorFontColor;;
  background: &dtml-manipulatorBackground;;
