<dtml-let root_url="get_root_url()" last_modified="_.DateTime()-14" expires="_.DateTime()+7">
/*
** _______ Silva stylesheet ___________________________________________________
**
** The view crew at Infrae - all seasons of 2003-2006
**   Thanks to Alexander Limi (limi@plone.org) - October 2002
**     In turn, thanks to Geir B�kholt, Stian S�iland for input and guidance.
**
** _______ Selector groups _______
**
** sectors
**   pathbar
**   matrix nav
**   flowbar
**   middleground
**   footer
**   personal bar
**   columns table
**   listing table
**   sidebar navigation
**   focus shapes
** form definitions
**   form control interaction
** info + clipboard
** feedback
** typography
** silva specific styles
**   align/float
**   lists
** links
** local styles
*/

<dtml-with stylesheet_properties>

body {
  margin: 0;
  padding: 0;
  color: &dtml-mainFontColor;;
  background-color: #9ab;
  background-image: url(<dtml-var root_url>/globals/skyrise.gif);
  background-repeat: repeat-x;
  font: &dtml-mainFont;;
}

/* _________________________________________________________ sectors _______ */

div.workspace {
  background: &dtml-workspace;;
  margin: 0;
}

/* ____________________________________ path bar _______ */

div.pathbar {
  border: none;
  padding: 0em 4px 0.2em 22px;
  color: &dtml-shade36;;
  background-image: url(<dtml-var root_url>/globals/skylight.gif);
  background-repeat: repeat-x;
  background-color: &dtml-pathBarBackground;;
  font-size: 90%;
  line-height: 1.4em;
}

div.pathbar a {
  margin: 0;
  color: &dtml-shade26;;
  font-weight: &dtml-mainTabFontWeight;;
}
div.pathbar a:visited {
  color: &dtml-shade26;;
}
div.pathbar a:hover {
  color: &dtml-shade42;;
  text-decoration: underline;
}

div.pathbar a.manage {
  margin-left: 0.5em ! important;
  color: &dtml-shade50;;
}

div.pathbar a.idea {
  color: &dtml-shade78;;
}
div.pathbar a.idea:visited {
  color: &dtml-shade78;;
}
div.pathbar a.idea:hover {
  color: &dtml-shade96;;
  text-decoration: none;
}

div.pathbar img {
  margin: 0 0 0 -22px;
  vertical-align: top;
}

/* __________________________________ matrix nav _______ */

div.tabs {
  display: block;
  height: 1.2em;
  margin: 0;
  padding: 0 8px 0 16px;
  border-width: 1px;
  border-style: none none solid none;
  border-bottom-color: #BCC2CA;
  background-image: url(<dtml-var root_url>/globals/skyrise.gif);
  background-repeat: repeat-x;
  background-color: &dtml-mainTabBackdrop;;
}

div.tabs a {
  /* The normal, unselected tabs. They are all links */
  float: left;
  display: block;
  width: 7.5em;
  height: 1.1em;
  margin: 0;
  padding: 0;
  border-color: &dtml-mainTabBorderColor;;
  border-width: 1px;
  border-style: solid solid solid solid;
  border-bottom-color: &dtml-shade50;;
  color: &dtml-mainTabFontColor;;
  background: &dtml-mainTabBackground;;
  background-image: url(<dtml-var root_url>/globals/reflection.png);
  background-position: top left;
  background-repeat: repeat-x;
  text-align: center;
  font-weight: &dtml-mainTabFontWeight;;
}

div.tabs a:visited {
  color: &dtml-mainTabFontColor;;
  background-color: &dtml-mainTabBackground;;
}

div.tabs a:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background-color: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
}

div.tabs a.selected {
  /* The selected tab. There's only one of these */
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.selected:visited {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.recede {
  border-bottom-color: &dtml-mainTabBackground;;
  /* The parent tab of a subscreen. There might be one of these :) */
  color: &dtml-mainTabFontColorSelected;;
  background-color: &dtml-mainTabBackgroundRecede;;
}

div.tabs a.recede:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  background-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.left {
  /* The left side of the tab. */
  width: 1.5em;
  border-style: solid none solid solid;
}

div.tabs a.left:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.right {
  /* The right side of the tab. */
  width: 1.5em;
  border-style: solid solid solid none;
}

div.tabs a.right:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs img {
  margin: 0.3em 0 0 0;
  border: none;
  vertical-align: bottom;
}

div.tabs div.air {
  display: block;
  float: left;
  width: 1.6em;
  height: 1em;
  background-color: transparent;
}

/* view public page button */
div.tabs a.transportlet {
  width: auto;
  padding-right:1em;
  padding-left:1.5em;
}

/* help (available as accesskey link) */
div.tabs a.normal {
  float: right;
  display: auto;
  width: auto;
  height: auto;
  border: none;
  color: transparent;
  background: transparent;
}

/* ____________________________________ flow bar _______ */

div.flowbar {
  margin: 0;
  padding: 0;
  border-top: 3px solid &dtml-mainTabBackgroundSelected;;
  border-bottom: 1px solid #6E8094;
  background: &dtml-mainTabBackgroundSelected;;
  clear:both;
}

/* ________________________________ middleground _______ */

div.middleground {
  margin: 0;
  padding: 0;
  border-bottom: 1px solid #657;
  background-color: #8797ab;
  background-image: url(<dtml-var root_url>/globals/reflection.png);
  background-position: top left;
  background-repeat: repeat-x;
  line-height:130%;
}

div.middleground a {
  /* The normal, unselected buttons. They are all links */
  margin: 0;
  padding: 0em 2em 0.1em;
  border: &dtml-buttonBorder;;
  border-style: &dtml-buttonBorderStyle;;
  background-image: url(<dtml-var root_url>/globals/reflection.png);
  background-position: top left;
  background-repeat: repeat-x;
  text-align: center;
  font-weight: &dtml-mainTabFontWeight;;
}
div.middleground a:link {
  color: &dtml-transporterFontColor;;
  background-color: #4d5680;
}
div.middleground a:visited {
  color: &dtml-transporterFontColor;;
  background-color: #4d5680;
}
div.middleground a:hover {
  border-color: #567;
  color: #e5fffe;
  background-color: #74748b;
  text-decoration: none;
}
div.middleground a:active {
  border-color: #789;
  color: #b3fffb;
  background-color: #8d97c2;
}

div.middleground a.selected {
  /* A selected, highlighted button. There should only be one of these. */
  color: #424244;
  background-color: #b6d6f3;
}

/* _______ middleground executor (action buttons) _______ */

div.middleground div.executor {
  float: right;
}

div.middleground div.executor a {
  color: &dtml-executorFontColor;;
  background-color: &dtml-executorBackground;;
}
div.middleground div.executor a:hover {
  color: #e5ffec;
  background-color: #538da3;
}
div.middleground div.executor a:active {
  color: #bfffd3;
  background-color: #64ABC8;
}

/* ______________________________________ footer _______ */

div.footer {
  clear: both;
  margin: 0.5em 0em 0em 0em;
  padding: 0em 16px 0.1em 16px;
  border-top: &dtml-shade60; 1px solid;
  border-bottom: none;
  color: &dtml-shade30;;
  background-color:#C4D6E7;
  font-size: 94%;
}

div.footer a {
  margin: 0;
  padding: 0;
  color: &dtml-mainLinkColor;;
  font-weight: &dtml-mainTabFontWeight;;
  text-decoration: none;
}
div.footer a:visited {
  color: &dtml-mainLinkColor;;
}

/* ________________________________ personal bar _______ */

div.personalbar {
  margin: 0em 0em 0em 0em;
  padding: 0.1em 16px 0.2em 16px;
  border-bottom: &dtml-shade30; 1px solid;
  background-color: #4c5c6c;
  text-align: center;
}

div.personalbar a {
  margin: 0;
  padding: 0;
  color: #b7cee6;
  text-decoration: none;
}
div.personalbar a:visited {
  color: #b7cee6;
}

div.personalbar img {
  vertical-align: middle;
}

/* _______________________________ columns table _______ */

table {
  font-size: 100%;/* for IE 5.x */
}

table th, table td {
  padding:0;
}

table.columns {
  width: 100%;
  padding: 0;
  border-spacing:0;
}

/* 3 column table (default) */
table.columns td.left {
  width: 20%;
  padding: 0 8px 0 8px;
  vertical-align: top;
}
table.columns td.main {
  width: 60%;
  padding: 0 7px 0 0;
  vertical-align: top;
}
table.columns td.right {
  width: 20%;
  padding: 0 8px 0 0;
  vertical-align: top;
}

/* 2 column table */
table.columns2 td.left {
  width: 20%;
  padding: 0 0 0 7px;
  vertical-align: top;
}
table.columns2 td.main {
  width: 80%;
  padding: 0 7px 0 8px;
  vertical-align: top;
}

/* 1 column */
table.columns1 td.main {
  width: 100%;
  padding: 0 7px 0 7px;
  vertical-align: top;
}

/* _______________________________ listing table _______ */

table.listing {
  width: 100%;
  margin: 0 0 1em 0;
  border: &dtml-listingBorder;;
  border-style: solid none solid solid;
  border-collapse: collapse;
  color: #444;
  font-family:"Lucida Grande", "Lucida Sans Unicode", verdana, arial, helvetica, sans-serif;
}

table.listing img {
  vertical-align: middle;
}

/* ______________ rows ______________ */

table.listing td.top {
  padding: 0;
  color: &dtml-listingTopHeadingColor;;
  background: &dtml-listingTopBackground;;
}

table.listing tr.top-controls td {
  padding: 0;
  background: &dtml-listingTopControlsBackground;;
}

table.listing tr.inter-controls td {
  padding: 0;
  color: #c9dfe2;
  background-color: &dtml-listingInterControlsBackground;;
}

table.listing tr.controls td {
  padding: 0;
  color: #c9dfe2;
  background-color: &dtml-listingControlsBackground;;
}

/* cycle text color in listing rows */
table.listing tr.odd td {
  background: &dtml-listingOddBackground;;
}
table.listing tr.even td {
  background: &dtml-listingEvenBackground;;
}

/* applies only to mozilla and IE7 */
table.listing tr.odd:hover td, table.listing tr.even:hover td {
  background-color: #e5f2ff;
}

tr.divider td, td.divider {
  height: 0.5em;
  background-color: &dtml-listingDividerBackground;; /* dark */
}

tr.separator td, td.separator {
  height: 0.5em;
  background-color: #c5d6e6; /* light */
}

/* ______________ cells ______________ */

table.listing th {
  padding: 0em 0.3em 0.1em;
  border-top: none;
  border-bottom: &dtml-listingBorder;;
  border-right: &dtml-listingBorder;;
  color: &dtml-listingHeaderFontColor;;
  background-color: &dtml-listingHeaderBackground;;
  font-weight: normal;
  text-align: left;
  vertical-align: middle;
  white-space: nowrap;
}

table.listing th.listing-left {
  width: 2%;
  padding-left: 0.2em;
  text-align: left;
}

table.listing th.listing-right {
  width:7%;
  text-align: right;
}

table.listing th.align-left, table.listing td.align-left {
  text-align: left;
}
table.listing th.align-center, table.listing td.align-center {
  text-align: center;
}
table.listing th.align-right, table.listing td.align-right {
  text-align: right;
}

table.listing td {
  padding: 0 0.3em;
  border-right: &dtml-listingBorder;;
  text-align: left;
  vertical-align: top;
}

/* title cell */
table.listing td.top h2 {
  margin: 0 0.2em 0.1em 0.3em;
  color: &dtml-listingTopHeadingColor;;
}
table.listing td.top img {
  vertical-align: top;
}

/* subhead cell */
table.listing tr.top-controls h3 {
  margin: 0.1em 0.3em 0.2em;
  padding: 0;
  color: &dtml-listingTopControlsHeadingColor;;
  line-height: 100%;
}

/* subsubhead cell */
table.listing tr.inter-controls h3 {
  margin: 0em 0.3em 0.1em;
  color: #dbe9f7; /* e3e9f4 */
}

table.listing td.listing-left {
  padding: 0 0.3em 0 0.2em;
  white-space: nowrap;
}

table.listing td.listing-right {
  padding: 0 0 0 0.2em;
  white-space: nowrap;
}
/* deprecated, use designator
table.listing td.field-title {
  text-align: right;
}
*/
/*
* some usability selectors:
* create a displayed block of the links, because then you can click on
* the whole tablecell and don't have to focus the text
*/

table#sortable tbody a, table.listing tbody a {
  display: block;
}
/* inline in designation descriptions */
table.listing tbody p a {
  display: inline;
}

table#sortable a.icon img, table.listing a.icon img {
  display: inline;
  float: left;
  margin-right: 0.5em;
}

/* reset the margin for the up/down arrows */
table#sortable td.listing-right img {
  margin: 0;
}

/* __________________________ sidebar navigation _______ */

table.listing th.top {
  padding: 0;
  border: &dtml-listingBorder;;
  border-bottom: none;
  color: #e3eef0;
  background: &dtml-listingHeaderBackground;;
  text-align: center;
}

table.listing th.top img {
  vertical-align: bottom;
  padding: 0;
}

table.listing th.top a.normal:link {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:visited {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:hover {
  display: block;
  color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
  text-decoration: none;
}

table.listing th.subth {
  color: #e9f5f7;
  background: #89a;
  text-align: center;
}

/* _______ uplinks _______ */
table.listing a.top {
  display: block;
  float: left;
  width: 1.4em;
  margin: 0;
  padding: 0.4em 0.2em 0;
  border-right: &dtml-listingBorder;;
  background: &dtml-listingHeaderBackground;;
}

table.listing a.up {
  display: block;
  float: right;
  width: 1.4em;
  margin: 0;
  padding: 0.4em 0.2em 0;
  border-left: &dtml-listingBorder;;
  background: &dtml-listingHeaderBackground;;
}

table.listing a.top:hover {
  border-style: solid solid none none;
  color: &dtml-mainTabBackgroundSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
  padding: 0 0.2em 0;
}

table.listing a.up:hover {
  border-style: solid none none solid;
  color: &dtml-mainTabBackgroundSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
  padding: 0 0.2em 0;
}

td.selected {
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: &dtml-listingBorder;;
  border-bottom: &dtml-listingBorder;;
  background: &dtml-oddRowBackground;;
}

td.unselected {
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: 1px solid &dtml-evenRowBackground;;
  border-bottom:  1px solid &dtml-evenRowBackground;;
  background: &dtml-evenRowBackground;;
}

td.unselected:hover {
  color: &dtml-mainTabFontColorSelected;;
  background-color: #d5e1ef;
  cursor: pointer;
}

table.listing td a.normal:link {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:visited {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:hover {
  display: block;
  color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
  text-decoration: none;
}

/* _______ slidelinks _______ */
table.listing a.left {
  display: block;
  float: left;
  width: 8px;
  height: 16px;
}

table.listing a.right {
  display: block;
  float: right;
  width: 8px;
  height: 16px;
}

table.listing a.navicon {
  display: block;
  float: left;
  width: 19px;
  height: 100%;
}

table.listing a.left:hover {
  background-image: url(<dtml-var root_url>/globals/slideleft.gif);
  background-color: transparent;
  text-decoration: none;
}

table.listing a.right:hover {
  background-image: url(<dtml-var root_url>/globals/slideright.gif);
  background-color: transparent;
  text-decoration: none;
}

div.normal {
  display: block;
}

/* ________________________________ focus shapes _______ */

form.sublisting {
  padding-top: 1em;
}

/* used to shape non-nested listing tables */
.focus-rectangle {
  width: 64em;
  margin: 0 7px;
}


/* ________________________________________________ form definitions _______ */

form {
  margin: 0;
  border: none;
}

input {
  margin: 1px 0;
  padding: 1px 2px;
  border: &dtml-inputBorder;;
  color: &dtml-inputFontColor;;
  background-color: &dtml-inputBackground;;
  font: &dtml-inputFont;;
}

textarea {
  margin: 1px 0;
  padding: 1px 2px;
  border: &dtml-inputBorder;;
  color: &dtml-inputFontColor;;
  background-color: &dtml-inputBackground;;
  font: &dtml-inputFont;;
}

input.store, textarea.store {
  width: 99%;
  !width: 94%; /* IE6 hack */
}

select {
  margin: 0 0 1px;
  border: &dtml-inputBorder;;
  color: &dtml-mainTabFontColorSelected;;
  background-color: &dtml-inputBackground;;
  font: &dtml-inputFont;;
  font-size: 95%;
  vertical-align: top;
}

select.store {
  width: 100%;
  !width: 95%; /* IE6 hack */
}

select.manipulator {
  margin: 0;
  border: &dtml-manipulatorBorder;;
  border-color: #064C29 #345 #064C29;
  color: &dtml-manipulatorFontColor;;
background-color:#195A3E;
}

select.transporter {
  margin: 0;
  border: &dtml-transporterBorder;;
  border-color: #283365 #345 #283365;
  color: &dtml-transporterFontColor;;
  background-color: &dtml-transporterBackground;;
}

/* for forms editor IE6 */
.fixIE6widget {
  width: 90%;
}

/* seems to keep IE7 in check without anything else */
input[type="submit"] {
  overflow: visible;
}

/* manipulator button style from which all others derive */
input.button, button.button, input.manipulator {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-buttonBorder;;
  border-style: &dtml-buttonBorderStyle;;
  color: &dtml-manipulatorFontColor;;
  background-color: &dtml-manipulatorBackground;;
  background-image: url(<dtml-var root_url>/globals/reflection.png);
  background-position: top left;
  background-repeat: repeat-x;
  cursor: &dtml-buttonCursor;;
}

/* used in conflict situations where public view css wins */
button.button { 
  font: &dtml-inputFont;;
}

input.transporter {
  color: &dtml-transporterFontColor;;
  background-color: &dtml-transporterBackground;;
}

input.executor {
  color: &dtml-executorFontColor;;
  background-color: &dtml-executorBackground;;
}

input.remover, input.warning {
  color: &dtml-removerFontColor;;
  background-color: &dtml-removerBackground;;
}

input.canceler {
  color: &dtml-cancelerFontColor;;
  background-color: &dtml-cancelerBackground;;
}

/* _______ state coded publishing action buttons _______ */
input.rejector {
  background-color: #8f7f01;
}
input.revoker {
  background-color: #008c55;
}
input.closer {
  background-color: #202080;
}

/* _______ form labels _______ */

label {
  color: #403f80;
}

tr.controls label, tr.inter-controls label {
  color: &dtml-listingHeaderFontColor;;
}

th label {
  color: &dtml-listingHeaderFontColor;;
}

/* _______ field iterators _______ */

table.listing td.designator {
  padding-top:0.1em;
  padding-bottom:0.1em;
}
table.listing td.designator label {
  font-size: 115%;
}
table.listing td.designator span.matchlabel {
  font-size: 115%;
}
table.listing td.designator p, td.designator ul,
table.listing td.field p {
  margin:0;
  color:#667;
}
table.listing tr:hover td.designator p, 
table.listing tr:hover td.designator ul:hover,
table.listing tr:hover td.field p {
  color:#333;
}
table.listing td.field {
  padding-top:0.1em;
  padding-right:0.4em;
}

/* _______ pseudo form button selectors, matches transporter button style _______ */

/* transport between screens */
a.transporter {
  margin: &dtml-pseudoButtonMargin;;
  padding: &dtml-pseudoButtonPadding;;
  border: &dtml-buttonBorder;;
  border-style: &dtml-buttonBorderStyle;;
  background-image: url(<dtml-var root_url>/globals/reflection.png);
  background-position: top left;
  background-repeat: repeat-x;
  font: &dtml-inputFont;;
  font-size: &dtml-pseudoButtonFontSize;;
  line-height: 130%;
}
a.transporter:link {
  color: &dtml-transporterFontColor;;
  background-color: &dtml-transporterBackground;;
}
a.transporter:visited {
  color: &dtml-transporterFontColor;;
  background-color: &dtml-transporterBackground;;
}
a.transporter:hover {
  border-color: #99b4cf;
  color: #e5fffe;
  background-color: #74748b;
  text-decoration: none;
}
a.transporter:active {
  color: #bfffd3;
  background-color: #008a8a;
}

/* _______ subscreen parent links _______ */

table.listing td.parent {
  padding: 0;
  border: &dtml-listingBorder;;
  background-color: &dtml-transporterBackground;;
  text-align: center;
  vertical-align: bottom;
}

table.listing td.parent a {
  color: &dtml-transporterFontColor;;
  text-decoration: none;
}
table.listing td.parent a:hover {
  color: #bfffd3;
  background-color: #53538b;
}
/* only used in lookup ui for back */
table.listing div.transport-border {
  padding-bottom: 1px;
  border: &dtml-transporterBorder;;
}
table.listing div.transport-border:hover {
  color: #bfffd3;
  background-color: #53538b;
}

/* _____________________________ form control interaction _______ */

/* _______ field interaction _______ */
/* these selectors are also define unclassed submit buttons */

input:hover, textarea:hover, select:hover {
  border-color : #599;
  color:#339;
  background-color: #def2f2;
}

input:focus, textarea:focus, select:focus {
  border-color : #0ff;
  color:#339;
