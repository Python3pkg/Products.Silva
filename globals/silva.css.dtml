/* ________________________________________________________ feedback _______ */

/* feedback under the matrix nav */
div.fixed-feedback {
  margin: 0;
  border-bottom: &dtml-listingBorder;;
  padding: 0em 9px 0.1em 9px;
  color: #555;
  background: #f3f3d9;
  border-collapse: collapse;
}
