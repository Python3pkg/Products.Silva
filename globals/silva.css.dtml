<dtml-let root_url="get_root_url()" last_modified="_.DateTime()-14" expires="_.DateTime()+7">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Content-Type', 'text/css' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Last-Modified', last_modified.toZone('GMT').rfc822() )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Cache-Control', 'max-age=36000, must-revalidate' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Expires', expires.toZone('GMT').rfc822() )">

/*
** _______ Silva stylesheet ___________________________________________________
**
** The view crew at Infrae - all seasons of 2003-2006
**   Thanks to Alexander Limi (limi@plone.org) - October 2002
**     In turn, thanks to Geir B�kholt, Stian S�iland for input and guidance.
**
** _______ Selector groups _______
**
** sectors
**   pathbar
**   matrix nav
**   flowbar
**   middleground
**   footer
**   personal bar
**   columns table
**   listing table
**   sidebar navigation
**   focus shapes
** form definitions
**   forms eye candy
** info + clipboard
** feedback
** typography
** silva specific styles
**   align/float
**   lists
** links
** local styles
*/

<dtml-with stylesheet_properties>

body {
  margin: 0;
  padding: 0;
  color: &dtml-mainFontColor;;
  background-color: &dtml-mainBackground;;
  background-image: url(<dtml-var root_url>/globals/skyrise.gif);
  background-repeat: repeat-x;
  font: &dtml-mainFont;;
}

/* _________________________________________________________ sectors _______ */

div.workspace {
  background: &dtml-workspace;;
  margin: 0;
}

/* ____________________________________ path bar _______ */

div.pathbar {
  border: none;
  padding: 0em 4px 0.2em 22px;
  color: &dtml-shade36;;
  background-image: url(<dtml-var root_url>/globals/skylight.gif);
  background-repeat: repeat-x;
  background-color: &dtml-pathBarBackground;;
  font-size: 90%;
  line-height: 1.4em;
}

div.pathbar a {
  margin: 0;
  color: &dtml-shade26;;
  font-weight: &dtml-mainTabFontWeight;;
}
div.pathbar a:visited {
  color: &dtml-shade26;;
}
div.pathbar a:hover {
  color: &dtml-shade42;;
  text-decoration: underline;
}

div.pathbar a.manage {
  margin-left: 0.5em ! important;
  color: &dtml-shade50;;
}

div.pathbar a.idea {
  color: &dtml-shade78;;
}
div.pathbar a.idea:visited {
  color: &dtml-shade78;;
}
div.pathbar a.idea:hover {
  color: &dtml-shade96;;
  text-decoration: none;
}

div.pathbar img {
  margin: 0 0 0 -22px;
  vertical-align: top;
}

/* __________________________________ matrix nav _______ */

div.tabs {
  display: block;
  height: 1.2em;
  margin: 0;
  padding: 0 16px 0 16px;
  border-width: 1px;
  border-style: none none solid none;
  border-bottom-color: &dtml-shade80;;
  background-image: url(<dtml-var root_url>/globals/skyrise.gif);
  background-repeat: repeat-x;
  background-color: &dtml-mainTabBackdrop;;
}

div.tabs a {
  /* The normal, unselected tabs. They are all links */
  float: left;
  display: block;
  width: 7.5em;
  height: 1.1em;
  margin: 0;
  padding: 0;
  border-color: &dtml-mainTabBorderColor;;
  border-width: 1px;
  border-style: solid solid solid solid;
  border-bottom-color: &dtml-shade50;;
  color: &dtml-mainTabFontColor;;
  background: &dtml-mainTabBackground;;
  text-align: center;
  font-weight: &dtml-mainTabFontWeight;;
}

div.tabs a:visited {
  color: &dtml-mainTabFontColor;;
  background: &dtml-mainTabBackground;;
}

div.tabs a:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
}

div.tabs a.selected {
  /* The selected tab. There's only one of these */
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.selected:visited {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.recede {
  border-bottom-color: &dtml-mainTabBackground;;
  /* The parent tab of a subscreen. There might be one of these :) */
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-mainTabBackgroundRecede;;
}

div.tabs a.recede:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
  background: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.left {
  /* The left side of the tab. */
  width: 1.5em;
  border-style: solid none solid solid;
}

div.tabs a.left:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

div.tabs a.right {
  /* The right side of the tab. */
  width: 1.5em;
  border-style: solid solid solid none;
}

div.tabs a.right:hover {
  border-bottom-color: &dtml-mainTabBackgroundSelected;;
}

/* help (available as accesskey link) */
div.tabs a.normal {
  display: none;
  float: right;
  width: 0em;
}

div.tabs img {
  margin: 0.3em 0 0 0;
  border: none;
  vertical-align: bottom;
}

div.tabs div.air {
  display: block;
  float: left;
  width: 2em;
  height: 1em;
  background: transparent;
}

/* ____________________________________ flow bar _______ */

div.flowbar {
  margin: 0;
  padding: 0;
  border-top: 3px solid &dtml-mainTabBackgroundSelected;;
  border-bottom: 1px solid #84a7a5;
  background: &dtml-mainTabBackgroundSelected;;
  clear:both;
}

/* ________________________________ middleground _______ */

div.middleground {
  display: block;
  height: 1.6em;
  margin: 0;
  padding: 0 17px 0 17px;
  border-bottom: 1px solid #456555;
  background-color: #677a8c;
  font-size: 105%;
}

div.middleground a {
  /* The normal, unselected buttons. They are all links */
  float: left;
  display: block;
  width: 12em;
  height: 1.3em;
  margin: 1px 1em 1px 1.5em;
  padding: 0;
  border-color: #8faecf;
  border-width: 1px;
  border-style: solid solid solid solid;
  text-align: center;
  font-weight: &dtml-mainTabFontWeight;;
}
div.middleground a:link {
  color: &dtml-transportFontColor;;
  background: #4d5680;
}
div.middleground a:visited {
  color: &dtml-transportFontColor;;
  background: #4d5680;
}
div.middleground a:hover {
  border-color: #99b4cf;
  color: #e5fffe;
  background: #74748b;
  text-decoration: none;
}
div.middleground a:active {
  color: #bfffd3;
  background: #008a8a;
}

div.middleground a.selected {
  /* A selected, highlighted button. There should only be one of these. */
  border-color: #3d4b67;
  color: #424244;
  background: #b6d6f3;
}

/* _______ middleground executor (publish now) _______ */

div.middleground a.executor {
  color: #b3fffb;
  background: #38656c;
}
div.middleground a.executor:hover {
  border-color: #8dc1a8;
  color: #e5ffec;
  background: #538da3;
}
div.middleground a.executor:active {
  color: #bfffd3;
  background: #008a8a;
}


/* ______________________________________ footer _______ */

div.footer {
  clear: both;
  margin: 0.5em 0em 0em 0em;
  padding: 0em 16px 0.1em 16px;
  border-top: &dtml-shade60; 1px solid;
  border-bottom: none;
  color: &dtml-shade30;;
  background: &dtml-listingTopBackground;;
  font-size: 94%;
}

div.footer a {
  margin: 0;
  padding: 0;
  color: &dtml-mainLinkColor;;
  font-weight: &dtml-mainTabFontWeight;;
  text-decoration: none;
}
div.footer a:visited {
  color: &dtml-mainLinkColor;;
}

/* ________________________________ personal bar _______ */

div.personalbar {
  margin: 0em 0em 0em 0em;
  padding: 0.1em 16px 0.2em 16px;
  border-bottom: &dtml-shade30; 1px solid;
  background: #4c5c6c;
  text-align: center;
}

div.personalbar a {
  margin: 0;
  padding: 0;
  color: #b7cee6;
  text-decoration: none;
}
div.personalbar a:visited {
  color: #b7cee6;
}

div.personalbar img {
  vertical-align: middle;
}

/* _______________________________ columns table _______ */

table {
  font-size: 100%;/* for IE 5.x */
}

table.columns {
  width: 100%;
  padding: 0;
  border-collapse: collapse;
}

table.columns tr {
  vertical-align: top;
}

/* 3 column table (default) */
table.columns td.left {
  width: 20%;
  padding: 0 8px 0 8px;
  vertical-align: top;
}
table.columns td.main {
  width: 60%;
  padding : 0 7px 0 0;
  vertical-align: top;
}
table.columns td.right {
  width: 20%;
  padding: 0 8px 0 0;
  vertical-align: top;
}

/* 2 column table */
table.columns2 td.left {
  width: 20%;
  padding: 0 0 0 7px;
  vertical-align: top;
}
table.columns2 td.main {
  width: 80%;
  padding: 0 7px 0 8px;
  vertical-align: top;
}

/* 1 column */
table.columns1 td.main {
  width: 100%;
  padding: 0 7px 0 7px;
  vertical-align: top;
}

/* _______________________________ listing table _______ */

table.listing {
  width: 100%;
  margin: 0em 0em 1em 0em;
  border-left: &dtml-listingBorder;;
  border-bottom: &dtml-listingBorder;;
  border-collapse: collapse;
  color: #444;
}

table.listing img {
  vertical-align: middle;
}

/* ______________ rows ______________ */

table.listing tr.top-controls {
  background: &dtml-listingTopControlsBackground;;
  /* in case it's empty */
  height: 0.2em;
}

table.listing tr.controls {
  text-align: right;
  /* in case it's empty */
  height: 0.2em;
  color: #c9dfe2;
  background: &dtml-listingControlsBackground;;
}

table.listing tr.odd {
  background: &dtml-listingOddBackground;;
}

/* applies only to mozilla comp. browsers */
td.main>table.listing tr.odd:hover td, td.main>table.listing tr.even:hover td {
  background-color: #e5f2ff;
}

table.listing tr.inter-controls td {
  color: #c9dfe2;
  background-color : #718191;
/*  background: &dtml-listingInterControlsBackground;; */
}

tr.divider, td.divider {
  height: 0.5em;
  background: &dtml-listingDividerBackground;; /* dark */
}

tr.separator, td.separator {
  height: 0.5em;
  background-color: #c5d6e6; /* light */
}

/* ______________ cells ______________ */

table.listing th {
  padding: 0em 0.6em;
  border-top: none;
  border-bottom: &dtml-listingBorder;;
  border-right: &dtml-listingBorder;;
  color: &dtml-listingHeaderFontColor;;
  background: &dtml-listingHeaderBackground;;
  font-weight: normal;
  text-align: left;
  vertical-align: middle;
  white-space: nowrap;
}

table.listing th.listing-left {
  width: 2%;
  padding-left: 0.4em;
  text-align: left;
}

table.listing th.listing-right {
  width: 4%;
  padding-right: 0.4em;
  text-align: right;
}

table.listing th.align-left, table.listing td.align-left {
  text-align: left;
}
table.listing th.align-center, table.listing td.align-center {
  text-align: center;
}
table.listing th.align-right, table.listing td.align-right {
  text-align: right;
}

table.listing td {
  padding: 0em 0.6em;
  border-right: &dtml-listingBorder;;
  text-align: left;
  vertical-align: top;
}

/* cycle text color */
table.listing tr.even td {
  color: #333;
  background: &dtml-listingEvenBackground;;
}

/* _______ title cell _______ */
table.listing td.top {
  padding: 0em;
  border: &dtml-listingBorder;;
  color: &dtml-shade40;;
  background: &dtml-listingTopBackground;;
}
table.listing td.top h2 {
  margin: 0.1em 0.4em;
  border: none;
  color: &dtml-listingTopHeadingColor;;
  text-align: left;
}
table.listing td.top img {
  vertical-align: top;
}

table.listing tr.top-controls td {
  padding: 0em 0.4em;
}

/* _______ subhead cell _______ */
table.listing tr.top-controls h3 {
  margin: 0.3em 0.2em 0.2em;
  padding: 0;
  border: none;
  color: &dtml-listingTopControlsHeadingColor;;
  line-height: 100%;
}

/* _______ subsubhead cell _______ */
table.listing tr.inter-controls h3 {
  color : #e3e9f4;
  margin: 0.2em 0.3em 0.1em;
}

table.listing td.listing-left {
  padding: 0em 0.4em;
  text-align: left;
  white-space: nowrap;
}

table.listing td.listing-right {
  text-align: right;
  white-space: nowrap;
}

table.listing tr.controls td, tr.inter-controls td {
  padding: 0 0.2em 0 0.4em;
}

table.listing td.field-title {
  text-align: right;
}

/* 
* some usability fixes
* create a displayed block of the links, because then you can click on
* the whole tablecell and don't have to focus the text
*/

table#sortable tbody a {
  display: block;
}

table#sortable a.icon img {
  float: left;
  margin-right: 0.5em;
}

/* reset the margin for the up/down arrows */
table#sortable td.listing-right a.icon img {
  margin: 0;
}

/* __________________________ sidebar navigation _______ */

table.listing th.top {
  padding: 0;
  border-top: &dtml-listingBorder;;
  border-bottom: none;
  color: #e3eef0;
  background: &dtml-listingHeaderBackground;;
  text-align: center;
}

table.listing th.top img {
  vertical-align: bottom;
  padding: 0;
}

table.listing th.top a.normal:link {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:visited {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:hover {
  display: block;
  color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
  text-decoration: none;
}

table.listing th.subth {
  color: #e9f5f7;
  background: #89a;
  text-align: center;
}

/* _______ uplinks _______ */
table.listing a.top {
  display: block;
  float: left;
  width: 1.4em;
  margin: 0;
  padding: 0.4em 0.2em 0;
  border-right: &dtml-listingBorder;;
  background: &dtml-listingHeaderBackground;;
}

table.listing a.up {
  display: block;
  float: right;
  width: 1.4em;
  margin: 0;
  padding: 0.4em 0.2em 0;
  border-left: &dtml-listingBorder;;
  background: &dtml-listingHeaderBackground;;
}

table.listing a.top:hover {
  border-style: solid solid none none;
  color: &dtml-mainTabBackgroundSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
  padding: 0 0.2em 0;
}

table.listing a.up:hover {
  border-style: solid none none solid;
  color: &dtml-mainTabBackgroundSelected;;
  background: &dtml-mainTabBackgroundSelected;;
  text-decoration: none;
  padding: 0 0.2em 0;
}

td.selected {
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: &dtml-listingBorder;;
  border-bottom: &dtml-listingBorder;;
  background: &dtml-oddRowBackground;;
}

td.unselected {
  padding-left: 0 ! important;
  padding-right: 0 ! important;
  border-top: 1px solid &dtml-evenRowBackground;;
  border-bottom:  1px solid &dtml-evenRowBackground;;
  background: &dtml-evenRowBackground;;
}

td.unselected:hover {
  color: &dtml-mainTabFontColorSelected;;
  background-color: #d5e1ef;
  cursor: pointer;
} 

table.listing td a.normal:link {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:visited {
  display: block;
  color: &dtml-mainTabFontColor;;
  background: &dtml-listingHeaderBackground;;
}

table.listing th.top a.normal:hover {
  display: block;
  color: &dtml-mainTabFontColorSelected;;
  background: #d2e6f8;
  text-decoration: none;
}

/* _______ slidelinks _______ */
table.listing a.left {
  display: block;
  float: left;
  width: 8px;
  height: 16px;
}

table.listing a.right {
  display: block;
  float: right;
  width: 8px;
  height: 16px;
}

table.listing a.navicon {
  display: block;
  float: left;
  width: 19px;
  height: 100%;
}

table.listing a.left:hover {
  background-image: url(<dtml-var root_url>/globals/slideleft.gif);
  background-color: transparent;
  text-decoration: none;
}

table.listing a.right:hover {
  background-image: url(<dtml-var root_url>/globals/slideright.gif);
  background-color: transparent;
  text-decoration: none;
}

div.normal {
  display: block;
}

/* ________________________________ focus shapes _______ */

form.sublisting {
  padding-top: 1em;
}

/* used to shape non-nested listing tables */
.focus-box {
  /* for IE5 */
  width: 73%;
  max-width: 56em;
  margin: 0 7px 0 7px;
}

.focus-rectangle {
  /* for IE5 */
  width: 96%;/* for IE6 */
  max-width: 98%;
  margin: 0 7px;
  padding-top: 7px;
}


/* ________________________________________________ form definitions _______ */

form {
  margin: 0;
  border: none;
  border-collapse: collapse;
}

input {
  margin-top: 1px;
  margin-bottom: 1px;/* IE bug fix */
  padding: 0.1em;
  border: &dtml-inputBorder;;
  color: &dtml-inputFontColor;;
  background: &dtml-inputBackground;;
  font: &dtml-inputFont;;
  vertical-align: middle;
}

textarea {
  padding: 0.1em;
  border: &dtml-inputBorder;;
  color: &dtml-inputFontColor;;
  background: &dtml-inputBackground;;
  font: &dtml-inputFont;;
}

input.store, select.store, textarea.store {
  width: 93%;
  margin-top: 1px;
  margin-bottom: 1px;/* IE bug fix */
}

/* 
* this selector works only for standard compatible browsers like mozilla, so
* IE ignores this definition below
*/
td>textarea.fixIE6widget, td>input.fixIE6widget[type=text], td>div.fixIE6widget, 
td>input.store, td>textarea.store, td>select.store {
  width: 99%;
}

select {
  margin: 0.1em;
  border: &dtml-inputBorder;;
  color: &dtml-mainTabFontColorSelected;;
  background: &dtml-inputBackground;;
  font: &dtml-inputFont;;
  font-size: 90%;
  line-height: 96%;
  vertical-align: top;
}

select.store {
  font-size: 90%;
  line-height: 96%;
}

optgroup {
  font-size: 100%;
}

select.transport {
  margin-top: 0.2em;
  border: &dtml-transportBorder;;
  color: &dtml-transportFontColor;;
  background: &dtml-transportBackground;;
}

select.manipulator {
  margin-top: 0.2em;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: &dtml-manipulatorBackground;;
}

input.button {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: &dtml-manipulatorBackground;;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}

button.button {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: &dtml-manipulatorBackground;;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}

input.transport {
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: &dtml-buttonPadding;;
  border: &dtml-transportBorder;;
  color: &dtml-transportFontColor;;
  background: &dtml-transportBackground;;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}

input.warning {
  margin: 0.2em 0.2em 0.1em 0.2em;
  padding: &dtml-buttonPadding;;
  border: &dtml-destructiveBorder;;
  color: &dtml-destructiveFontColor;;
  background: &dtml-destructiveBackground;;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}

input.cancel {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-cancelBorder;;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  color: &dtml-cancelFontColor;;
  background: &dtml-cancelBackground;;
  cursor: &dtml-bottonCursor;;
}

input.executor {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: 1px solid #6bb591;
  color: #b3fffb;
  background: #21565b;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}
input.executor[type="submit"]:hover {
  border-color: #8dc1a8;
  color: #e5ffec;
  background: #538da3;
}

/* publish screen state coded buttons */
input.reject {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: #8f7f01;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}
input.revoke {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: #008c55;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}
input.close {
  margin: &dtml-buttonMargin;;
  padding: &dtml-buttonPadding;;
  border: &dtml-manipulatorBorder;;
  color: &dtml-manipulatorFontColor;;
  background: #202080;
  font-size: &dtml-buttonFontSize;;
  font-weight: &dtml-buttonWeight;;
  cursor: &dtml-bottonCursor;;
}


/* _______ form labels _______ */

label {
  color: #403f80;
}

tr.controls label, tr.inter-controls label {
  color: &dtml-listingHeaderFontColor;;
}

th label {
  color: &dtml-listingHeaderFontColor;;
}


/* _______ pseudo form button selectors, matches button style _______ */

table.listing a.button:link {
  border: &dtml-manipulatorBorder;;
  margin: 0.1em 0.2em;
  padding: 0em 1em 0.1em 1em;
  color: &dtml-manipulatorFontColor;;
  background: &dtml-manipulatorBackground;;
