/* ________________________________________________ info + clipboard _______ */

div.info {
  margin: 0;
  padding: 0.2em 0.4em 0.4em;
  border: &dtml-listingBorder;;
  color: &dtml-inputFontColor;;
  background: &dtml-evenRowBackground;;
}

div.info h3 {
  margin-top: 0.2em;
  margin-bottom: 0.2em;
  color: &dtml-inputFontColor;;
}

div.info h3 img {
  vertical-align:top;
}

div.info p {
  margin-top: 0.6em;
  margin-bottom: 0em;
}

div.info p span.orientation {
  display: block;
  margin-top: 0.3em;
  border: 1px dotted black;
  text-align: center;
}

div.info p span.portrait {
  width: 4.5em;
  height: 6em;
}

div.info p span.landscape {
  width: 6.5em;
  height: 3.5em;
}

div.info p span.square {
  width: 4em;
  height: 4em;
}

select.clipboard, select.clipboard:focus, select.clipboard:hover {
  width: 100%;
  max-width: 100%;
  margin-top: 0.1em;
  margin-bottom: -0.5em;
  border: 1px solid &dtml-shade60;;
  color: &dtml-shade26;;;
  background: &dtml-listingEvenBackground;;
  font-size: 90%;
  line-height: 96%;
}

select.clipboard option.disabled {
  color: #dbf3f6;
  background: &dtml-listingHeaderBackground;;
}


/* ________________________________________________________ feedback _______ */

/* feedback under the matrix nav */
div.fixed-feedback {
  margin: 0;
  border-bottom: &dtml-listingBorder;;
  padding: 0em 9px 0.1em 9px;
  color: #555;
  background: #f3f3d9;
  border-collapse: collapse;
}
div.fixed-alert {
  height: 1%; /*hack for ie */
  margin: 0;
  border-bottom: &dtml-listingBorder;;
  padding: 0em 9px 0.1em 9px;
  color: #3c2d50;
  background: #f7d1d1;
  border-collapse: collapse;
}
div.fixed-clear {
  margin: 0;
  padding: 0em 9px 0.1em 9px;
  border-bottom: 1px solid &dtml-workspace;;
  background: transparent;
  border-collapse: collapse;
}
div.fixed-clear a.reveal {
  color: &dtml-shade96;;
}
div.fixed-clear a.reveal:visited {
  color: &dtml-shade96;;
}
div.fixed-clear a.reveal:hover {
  color: &dtml-shade58;;
  text-decoration: none;
}

/* for notifications in right column */
td.right div.feedback {
  margin-top: 0;
}

/* floating feedback  */
div.feedback {
  position:relative; /* for IE6 peekaboo bug */
  margin: 0;
  border: &dtml-listingBorder;;
  padding: 0.4em 0.7em 0.7em 0.7em;
  color: #555;
  background-color: #f3f3d9;
}
div.alert {
  margin: 0;
  border: &dtml-listingBorder;;
  padding: 0.4em 0.7em 0.7em 0.7em;
  color: #3c2d50;
  background-color: #f7d1d1;
}
div.feedback a {
  color: &dtml-mainLinkColor;;
  text-decoration: underline;
}

/* table row/cell feedback  */
table.listing tr.feedback, table.listing td.feedback {
  padding-top: 0.1em;
  padding-bottom: 0.2em;
  border-top: &dtml-listingBorder;;
  color: #555;
  background-color: #f3f3d9;
}
table.listing tr.alert, table.listing td.alert {
  border-top: &dtml-listingBorder;;
  border-bottom: &dtml-listingBorder;;
  color: #3c2d50;
  background-color: #f7d1d1;
}


/* _______ message box in content publish screen _______ */

table.listing .message {
  padding: 0;
}

table.listing div.message {
  margin: 0;
  padding: 0.2em 0.6em 0.2em 0.6em;
  border: &dtml-messageBorder;;
  border-style: solid none solid none;
  color: #2e2e77;
  background-color: &dtml-messageBackground;;
  font-weight: normal;
}


