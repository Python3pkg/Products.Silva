<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>add_course_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>
  <i18n_domain>silva</i18n_domain>
  <i18n>form_add_link</i18n>

  <groups multiple="true">
    <group>
      <title>default</title>
      <fields multiple="true">

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>No spaces or special characters besides &amp;#8216;_&amp;#8217; or &amp;#8216;-&amp;#8217; or &amp;#8216;.&amp;#8217;</description>
          <extra>id="object_id"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>title</title>
          <description>The title will be publicly visible, and is used for the link in indexes.</description>
          <extra>id="object_title"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      <field><id>link_type</id> <type>RadioField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>absolute</default>
          <description>Links can contain anything, so it is the author's responsibility to create valid urls.</description>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra_item></extra_item>
          <first_item type="int">0</first_item>
          <hidden type="int">0</hidden>
          <items type="list">[('absolute', 'absolute'), ('relative', 'relative')]</items>
          <orientation>vertical</orientation>
          <required type="int">1</required>
          <title>link type</title>
          <extra></extra>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
      </field>

      <field><id>url</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">40</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>url</title>
          <truncate type="int">0</truncate>
          <description>Link url.</description>
          <extra>id="url"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      </fields>
    </group>
  </groups>
</form>
