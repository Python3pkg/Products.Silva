<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>add_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>
  <i18n_domain>silva</i18n_domain>
  <i18n>form_add_folder</i18n>

  <groups multiple="true">
    <group>
      <title>default</title>
      <fields multiple="true">

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>No spaces or special characters besides &amp;#8216;_&amp;#8217; or &amp;#8216;-&amp;#8217; or &amp;#8216;.&amp;#8217;</description>
          <extra></extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_title</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>title</title>
          <description>The title will be publicly visible, and is used for the link in indexes.</description>
          <extra></extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>policy_name</id> <type>ListField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class>store</css_class>
          <description>Choose an item to be created within the folder.</description>
          <external_validator></external_validator>
          <extra></extra>
          <extra_item></extra_item>
          <hidden type="int">0</hidden>
          <items type="list">[('Silva Document', 'Silva Document'), ('Automatic Table of Contents', 'Auto TOC'), ('None', 'None')]</items>
          <required type="int">1</required>
          <size type="int">5</size>
          <title>first item</title>
          <unicode type="int">0</unicode>
          <view_separator><br /></view_separator>
          <first_item type="int">1</first_item>
        </values>
        <tales>
              <items type="list">python:form.service_containerpolicy.listAddablePolicies(form.REQUEST.model)</items>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="unknown_selection">You selected an item that was not in the list.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
