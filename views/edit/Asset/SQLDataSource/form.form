<?xml version="1.0" encoding="latin-1"?>
<form>
  <title></title>
  <name>edit_sql_object_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups multiple="true">
    <group>
      <title>Default</title>
      <fields multiple="true">
        <field><id>object_title</id> <type>TextAreaField</type>
          <values>
            <alternate_name></alternate_name>
            <hidden type="int">0</hidden>
            <max_linelength></max_linelength>
            <max_length></max_length>
            <max_lines></max_lines>
            <width type="int">40</width>
            <external_validator></external_validator>
            <height type="int">2</height>
            <required type="int">1</required>
            <css_class></css_class>
            <default></default>
            <title>Title</title>
            <description>The title will be publicly visible, and is used as the default link in indexes.</description>
            <extra></extra>
          </values>
          <tales>
            <default>python:form.get_title()</default>
          </tales>
        </field>
      <field><id>connection_id</id> <type>ListField</type>
        <values>
          <size type="int">5</size>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <external_validator></external_validator>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>connection_id</title>
          <first_item type="int">0</first_item>
          <description>Select the id of the database connection to use</description>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.get_connection_id()</default>
          <items>python:form.connection_ids()</items>
        </tales>
      </field>
      <field><id>parameters</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">5</height>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>Query parameters</title>
          <description>
            Parameter definitions for query, each on a new line. &lt;br /&gt; 
            Syntax:&lt;code&gt; type : name : default value : description &lt;/code&gt; 
          </description>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.get_parameters()</default>
        </tales>
      </field>
      <field><id>statement</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">7</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>SQL Query</title>
          <description>
            SQL Query. See "Z SQL Methods" for syntax. &lt;br /&gt;
            Pay special attention to &lt;code&gt; &amp;lt;dtml-var&amp;gt; 
            &lt;/code&gt; vs. &lt;code&gt; &amp;lt;dtml-sqlvar&amp;gt; 
            &lt;/code&gt; usage.
          </description>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.get_statement()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>