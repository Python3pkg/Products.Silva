<?xml version="1.0" encoding="UTF-8"?>
<form>
  <title></title>
  <name>edit_sql_object_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups multiple="true">
    <group>
      <title>Default</title>
      <fields multiple="true">
      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <truncate type="int">0</truncate>
          <description></description>
          <extra>style="width:100%;"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
          <default>python: form.REQUEST.model.get_title()</default>
        </tales>
      </field>
      <field><id>connection_id</id> <type>ListField</type>
        <values>
          <size type="int">5</size>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <external_validator></external_validator>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Connection id</title>
          <first_item type="int">0</first_item>
          <description>Select the id of the database connection to use.</description>
          <extra></extra>
        </values>
        <tales>
          <default>python: form.get_connection_id()</default>
          <items>python: form.connection_ids()</items>
        </tales>
      </field>
      <field><id>data_encoding</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">0</required>
          <css_class></css_class>
          <default>ascii</default>
          <title>Data encoding</title>
          <truncate type="int">0</truncate>
          <description></description>
          <extra>style="width:100%;"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
          <default>python: form.REQUEST.model.get_data_encoding()</default>
        </tales>
      </field>
      <field><id>parameters</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">5</height>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>Query parameters</title>
          <description>
            Parameter definitions for query, each on a new line. &lt;br /&gt; 
            Syntax: &lt;code&gt;type : name : default value : description&lt;/code&gt; 
          </description>
          <extra>style="width:100%;"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
          <default>python: form.get_parameters()</default>
        </tales>
      </field>
      <field><id>statement</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">7</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>SQL Query</title>
          <description>
            See "ZSQL Methods" for syntax. &lt;br /&gt;
            Pay special attention to &lt;code&gt;&amp;lt;dtml-var&amp;gt;&lt;/code&gt; vs. 
            &lt;code&gt;&amp;lt;dtml-sqlvar&amp;gt;&lt;/code&gt; usage.
          </description>
          <extra>style="width:100%;"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
          <default>python: form.get_statement()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
