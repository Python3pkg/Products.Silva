<?xml version="1.0"?>

<form>
  <title></title>
  <row_length>4</row_length>
  <name>crop</name>
  <action></action>
  <method>POST</method>
  <enctype></enctype>
  <encoding>UTF-8</encoding>
  <stored_encoding>ISO-8859-1</stored_encoding>
  <unicode_mode>false</unicode_mode>
  <i18n_domain>silva</i18n_domain>
  <i18n>crop</i18n>
  <groups>
    <group>
      <title>Default</title>
      <fields>

        <field><id>size</id> <type>ListField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default type="list">[]</default>
          <description>Crop the image with the specified dimensions.</description>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <extra_item></extra_item>
          <hidden type="int">0</hidden>
          <items type="list">[('25x25', '25x25'), ('50x50', '50x50'), ('75x75', '75x75'), ('100x100', '100x100')]</items>
          <required type="int">1</required>
          <size type="int">1</size>
          <title>dimensions</title>
          <unicode type="int">0</unicode>
          <view_separator></view_separator>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="unknown_selection">You selected an item that was not in the list.</message>
        </messages>
      </field>
      <field><id>x</id> <type>IntegerField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>0</default>
          <description>The x coordinate of the center of the area to crop.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">12</display_width>
          <enabled type="int">1</enabled>
          <end></end>
          <external_validator></external_validator>
          <extra>id="x"</extra>
          <hidden type="int">0</hidden>
          <required type="int">1</required>
          <start></start>
          <title>x</title>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="not_integer">You did not enter an integer.</message>
          <message name="integer_out_of_range">The integer you entered was out of range.</message>
        </messages>
      </field>
      <field><id>y</id> <type>IntegerField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>0</default>
          <description>The y coordinate for cropping.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">12</display_width>
          <enabled type="int">1</enabled>
          <end></end>
          <external_validator></external_validator>
          <extra>id="y"</extra>
          <hidden type="int">0</hidden>
          <required type="int">1</required>
          <start></start>
          <title>y</title>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="not_integer">You did not enter an integer.</message>
          <message name="integer_out_of_range">The integer you entered was out of range.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
