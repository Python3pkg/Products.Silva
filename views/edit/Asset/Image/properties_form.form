<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>image_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <display_width type="int">41</display_width>
          <unicode type="int">0</unicode>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <truncate type="int">0</truncate>
          <description>A descriptive title pays off in the future.</description>
          <extra></extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      </fields>
    </group>
    <group>
      <title>Web presentation</title>
      <fields>

      <field><id>web_format</id> <type>RadioField</type>
        <values>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <external_validator></external_validator>
          <required type="int">1</required>
          <items type="list">[('jpg', 'JPEG'), ('png', 'PNG'), ('gif', 'GIF')]</items>
          <default></default>
          <title>Web format</title>
          <first_item type="int">1</first_item>
          <alternate_name></alternate_name>
          <unicode type="int">0</unicode>
          <description>Image format for web presentation.</description>
          <orientation>horizontal</orientation>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="unknown_selection">You selected an item that was not in the list.</message>
        </messages>
      </field>
      <field><id>web_scaling</id> <type>StringField</type>
        <values>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <required type="int">1</required>
          <css_class></css_class>
          <default>100%</default>
          <title>Scaling</title>
          <alternate_name></alternate_name>
          <unicode type="int">0</unicode>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <extra></extra>
          <truncate type="int">0</truncate>
          <description>Image scaling for web presentation. WidthXheight or percentage (100x200, 40%)</description>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
