<div class="margins"
    tal:define="model request/model">
<p class="halfline">&nbsp;</p>
<content tal:condition="model/get_unapproved_version">
    <h2><span tal:replace="model/id" /></h2>

<form action="submit" method="POST">
<table width="96%" cellspacing="0" cellpadding="5" border="0"
  tal:attributes="width container/table_width; cellspacing container/table_cellspacing;
                  cellpadding container/table_cellpadding; border container/table_border">
<tr>
<th align="left" colspan="2">
Attributes
</th>
<th align="left">
Content
</th>
<th align="left">
Notes
</th>
</tr>

<tr class="focus" valign="top">
<td align="right">
Url:
</td>
<td align="right">
<span class="required">*</span>
</td>
<td>
<input type="text" name="content_url" size="60" value="" 
  tal:attributes="value python:model.get_editable().get_content_url()"/>
</td>
<td class="leadedbody">
Url: location of originating content.
</td>
</tr>

<tr class="focus-cycle" valign="top"
 tal:condition="python:model.get_editable().get_content_url()">
<td align="right">
Link:
</td>
<td align="right">
&nbsp;
</td>
<td>
<a href="original"
  tal:attributes="href python:model.get_editable().get_content_url() + '/edit/tab_preview'"
  tal:content="structure python:model.get_editable().get_content_url() + ' &nbsp;(' + model.get_editable().get_title_html() + ')'">Link
</a>
</td>
<td class="leadedbody">
&nbsp;
</td>
</tr>

<tr class="controls">
<td colspan="2">
<input class="cancelbutton" type="button" name="edit_cancel" value=" Cancel " onClick="history.back()" />
</td>
<td align="right" colspan="2">
<input class="button" type="submit" name="add_submit" value=" Save ghost " />
</td>
</tr>
</table>
</form>
</content>
</div>