<html tal:omit-tag=""
  lang="en"
  xml:lang="en"
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="silva"
  tal:define="
    model request/model;
    root_url model/get_root_url;
    title model/get_title;
    vein string:edit;
">
<table class="columns columns3">
  <tbody>
    <tr>
      <tal:block replace="nothing">
        _____________________ sidebar _____________________
      </tal:block>
      <td class="left"
        tal:define="template_id string:tab_edit">
        <metal:block use-macro="here/macro_vein_navigation/macros/vein" />
      </td>

      <tal:block replace="nothing">
        _____________________ main _____________________
      </tal:block>
      <td class="main">

<tal:block
  tal:repeat="version_rec here/edit_info_get_versions">
  <tal:block tal:content="structure python:here.edit_info_showversion(**version_rec)">
    version information
  </tal:block>
</tal:block>

      </td>
    </tr>
  </tbody>
</table>
</html>

