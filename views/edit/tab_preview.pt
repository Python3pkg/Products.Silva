<!-- ___________________________ start index_html ___________________________ -->
<tal:block define="global breadcrumb_vein string:Preview;" />
<html metal:use-macro="here/macro_index/macros/master">
<div metal:fill-slot="main" tal:omit-tag="">
<!-- ___________________________ start index_html/slot main ___________________________ -->

<tal:block condition="exists: model/layout_macro.html/macros/previewer"
  tal:on-error="structure string:<div class='alert'>There is a problem in layout_macro.html, or in a script that's called in it. Use the 'Presentation preview' link to see what's wrong.</div>">
<metal:preview use-macro="model/layout_macro.html/macros/previewer">

<span tal:replace="nothing"> _______ closed message _______ </span>
<div class="feedback" style="width:70%;" tal:condition="here/tab_preview_needs_feedback">
  <b class="warning">This is a closed version. You see it, but the public won't!</b>
</div>

<!-- start Silva content -->
<div metal:fill-slot="main" tal:omit-tag="" 
  tal:content="structure model/preview">
  <!-- this comment gets replaced by the Silva document content -->
</div>
<!-- end Silva content -->

</metal:preview>

</tal:block>

<table border="0" width="100%" cellspacing="0" cellpadding="0"
  tal:condition="not: exists: model/layout_macro.html/macros/previewer">

<tr valign="top">
  <td width="32" bgcolor="#d1d4d8">
    <img tal:attributes="src here/globals/silva_icon_32.gif/absolute_url" height="32" width="32" />
  </td>
  <td width="16" bgcolor="#dde1e4">
    <img tal:attributes="src here/globals/pixel.gif/absolute_url" height="1" width="16" />
  </td>
  <td width="12">
    <img tal:attributes="src here/globals/pixel.gif/absolute_url" height="1" width="12" />
  </td>
  <td valign="top">
    <span tal:replace="nothing"> _______ link to see page in public/local style _______ </span>
    <span class="widebody"><a tal:attributes="href string:${model/absolute_url}/preview_html" target="_blank" style="float:right;">Presentation&nbsp;preview</a></span>
    <span tal:replace="nothing"> _______ closed message _______ </span>
    <div class="feedback" style="width:70%;" tal:condition="here/tab_preview_needs_feedback">
      <b class="warning">This is a closed version. You see it, but the public won't!</b>
    </div>
  </td>
  <td width="4%">
    <img tal:attributes="src here/globals/pixel.gif/absolute_url" height="1" width="16" />
  </td>
</tr>

<tr valign="top">
  <td bgcolor="#d1d4d8">
    &nbsp;
  </td>
  <td bgcolor="#dde1e4">
    &nbsp;
  </td>
  <td>
    <img tal:attributes="src here/globals/pixel.gif/absolute_url" height="1" width="12" />
  </td>
  <span tal:replace="nothing"> _______ content cell, here the XML is rendered in HTML _______ </span>
  <td valign="top">
    <div class="readable" tal:content="structure model/preview">
    <!-- this text is replaced by Silva content -->
    </div>
    <br />
  </td>
  <td>
    <img tal:attributes="src here/globals/pixel.gif/absolute_url" height="1" width="16" />
  </td>
</tr>
</table>
<!-- ___________________________ end index_html/slot main ___________________________ -->
</div>
</html>
<!-- ___________________________ end index_html ___________________________ -->
