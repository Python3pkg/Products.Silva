<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html note="insert properties middleground"
  tal:omit-tag=""
  lang="en"
  xml:lang="en"
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:notal="http://www.infrae.com/silva/notal"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="silva">

<metal:def-properties define-macro="properties">
<div class="middleground">
  <div style="float:right">
    <tal:def define="unapproved_version model/get_unapproved_version | nothing">
      <a class="executor"
        title="publish this document: alt-p"
        accesskey="p"
        tal:attributes="href string:${model/absolute_url}/edit/quick_publish?return_to=tab_metadata"
        i18n:translate=""
        i18n:attributes="title"
        tal:condition="python: unapproved_version and may_approve_content">
        publish&nbsp;now
      </a>
    </tal:def>
  </div>
  <a title="various settings: alt-e"
    accesskey="e"
    tal:define="
      link_id string:tab_settings;
      selected python:link_id == template.id;
      class python:selected and 'selected' or nothing;
    "
    tal:attributes="
      href string:$url/edit/$link_id;
      class class;
      "
    i18n:attributes="title">
    <tal:i18n i18n:translate="">settings</tal:i18n><tal:if condition="not: selected">...</tal:if>
  </a><tal:if condition="may_approve_content"><a title="manage subscriptions: alt-u"
      accesskey="u"
      tal:define="
        link_id string:tab_subscriptions;
        selected python:link_id == template.id;
        class python:selected and 'selected' or nothing;
      "
      tal:condition="context/is_subscribable_content"
      tal:attributes="
        href string:$url/edit/$link_id;
        class class;
      "
      i18n:attributes="title">
      <tal:i18n i18n:translate="">subscriptions</tal:i18n><tal:if condition="not: selected">...</tal:if>
    </a
    ><a title="change the addables allowed in this publication: alt-d"
      accesskey="d"
      tal:define="
        link_id string:tab_addables;
        selected python:link_id == template.id;
        class python:selected and 'selected' or nothing;
        ispublication python: model.implements_publication();
      "
        tal:condition="ispublication"
      tal:attributes="
        href string:$url/edit/$link_id;
        class class;
      "
      i18n:attributes="title">
      <tal:i18n i18n:translate="">addables</tal:i18n><tal:if condition="not: selected">...</tal:if>
    </a>
  </tal:if>
</div>
</metal:def-properties>

<metal:def-access define-macro="access">
<div class="middleground">
  <a class="manipulator"
    title="external users: alt-e"
    accesskey="e"
    tal:attributes="href string:../@@tab_externalusers"
    i18n:translate=""
    i18n:attributes="title">
    manage external users...
  </a
  ><a class="manipulator"
    title="lookup users: alt-l"
    accesskey="l"
    tal:attributes="href string:${context/absolute_url}/edit/lookup"
    i18n:translate=""
    i18n:attributes="title">
    lookup users...
  </a>
</div>
</metal:def-access>



</html>
